<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.student.crm.mapper.SystemDictionaryMapper" >
  <resultMap id="BaseResultMap" type="com.student.crm.domain.SystemDictionary" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="sn" property="sn" jdbcType="VARCHAR" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="intro" property="intro" jdbcType="VARCHAR" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Long" >
    delete from systemdictionary
    where id = #{id,jdbcType=BIGINT}
  </delete>
  <insert id="insert" parameterType="com.student.crm.domain.SystemDictionary" useGeneratedKeys="true" keyProperty="id" >
    insert into systemdictionary (sn, name, intro
      )
    values (#{sn,jdbcType=VARCHAR}, #{name,jdbcType=VARCHAR}, #{intro,jdbcType=VARCHAR}
      )
  </insert>
  <update id="updateByPrimaryKey" parameterType="com.student.crm.domain.SystemDictionary" >
    update systemdictionary
    set sn = #{sn,jdbcType=VARCHAR},
      name = #{name,jdbcType=VARCHAR},
      intro = #{intro,jdbcType=VARCHAR}
    where id = #{id,jdbcType=BIGINT}
  </update>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long" >
    select id, sn, name, intro
    from systemdictionary
    where id = #{id,jdbcType=BIGINT}
  </select>
  <select id="selectAll" resultMap="BaseResultMap" >
    select id, sn, name, intro
    from systemdictionary
  </select>
    <select id="queryPageCount" resultType="java.lang.Long">
        select count(id) from systemdictionary
    </select>
    <select id="queryPageData" resultType="com.student.crm.domain.SystemDictionary">
        select * from systemdictionary
        limit #{start},#{rows}
    </select>
</mapper>